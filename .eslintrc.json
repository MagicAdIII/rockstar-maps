{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType" : "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "globals": {
        "$": true
    },
    "rules": {
        "no-unused-vars": 1,
        "no-console": 0
    }
}